class Solution{
  public:
    void swapElements(int arr[], int sizeof_array){
        
        int temp=0,i;
        for(i=0;i<sizeof_array-2;i++)
        {
            //if(i+2<sizeof_array)
            //{
            //swap(arr[i],arr[i+2]);
            //}
            temp=arr[i];
            arr[i]=arr[i+2];
            arr[i+2]=temp;
        }
        
    }
};
